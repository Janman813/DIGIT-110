start = xml
xml = element xml{song}
song = element song { mixed{title?, artist?, verse+, chorus+}}
title = element title {text}
artist = element artist {type, text}
verse = element verse {n+, mixed{line+, text}, type?}
chorus = element chorus {n+, line*, type, text}
line = element line {n+, text} 
type = attribute type {"mezzoSoprano" | "end" | "start"}
n = attribute n {xsd:integer}
